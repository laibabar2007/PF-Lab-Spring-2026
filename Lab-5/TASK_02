#include<stdio.h>
#include<string.h>
int main()
{
int unit,bill;
char optn[20];
printf("Enter total units consumed");
scanf("%d",&unit);
if(unit<=100)
{
bill=10*unit;
printf("Total bill is %d units",bill);
}
else
{
if(unit>100 && unit<=300)
{
printf("Enter your customer type:\n");
scanf("%s",&optn);
if(strcmp(optn,"domestic")==0)
       {
        bill=unit*12;
    printf("Total bill is %d units",bill);
      }
    else
{
if(strcmp(optn,"commercial")==0)
{
    bill=unit*15;
   printf("Total bill is %d units",bill);
   }
   else
   {
    printf("Invalid optn");
}
}}
return 0;
}}
