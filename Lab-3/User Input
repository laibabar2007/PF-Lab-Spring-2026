#include <stdio.h>

int main()
{
    int a;
    float b;
    char c;

    printf("Enter an integer: ");
    scanf("%d", &a);

    printf("Enter a float: ");
    scanf("%f", &b);

    printf("Enter a character: ");
    scanf(" %c", &c);

    printf("\nInteger: %d\n",a);
    printf("Float: %f\n", b);
    printf("Character: %c\n", c);

    return 0;
}
